<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE form-validation PUBLIC
     "-//Apache Software Foundation//DTD Commons Validator Rules Configuration 1.3.0//EN"
     "http://jakarta.apache.org/commons/dtds/validator_1_3_0.dtd">
<form-validation>
	<formset>
		<form name="loginForm">
			<field property="nombre_usuario" depends="required">
				<arg key="form.name"/>
			</field>		
			
			<field property="clave_usuario" depends="required">
				<arg key="form.clave"/>
			</field>
		</form>
		<form name="agregarProyectoForm"></form>
		<form name="editarProyectoForm"></form>
		<form name="eliminarProyectoForm"></form>
		<form name="agregarUsuarioForm"></form>
		<form name="eliminarUsuarioForm"></form>
		<form name="editarUsuarioForm"></form>
		<form name="agregarMiembroForm"></form>
		<form name="cambiarEstadoForm"></form>
		<form name="editarItemForm"></form>
		<form name="eliminarItemForm"></form>
		<form name="agregarItemForm"></form>
		<form name="agregarItemForm"></form>
		<form name="agregarTipoItemForm"></form>
		<form name="eliminarEstadoSiguienteForm"></form>
		<form name="eliminarMiembroDisponibleForm"></form>
	 	<form name="eliminarTipoItemForm"></form>
		<form name="editarTipoItemForm"></form>
		<form name="agregarEstadoTipoItemForm"></form>
		<form name="eliminarEstadoTipoItemForm"></form>
		<form name="agregarEstadoSiguienteForm"></form>
		<form name="agregarMiembroDisponibleForm"></form>
		<form name="editarMiembroForm"></form>
	</formset>
</form-validation>